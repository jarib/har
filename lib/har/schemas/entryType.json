{
  "id": "entryType",
  "description": "Request and Response related info",
  "properties": {
    "pageref": {
      "type": "string"
    },
    "startedDateTime": {
      "type": "string",
      "format": "date-time",
      "pattern": "^(\\d{4})(-)?(\\d\\d)(-)?(\\d\\d)(T)?(\\d\\d)(:)?(\\d\\d)(:)?(\\d\\d)(\\.\\d+)?(Z|([+-])(\\d\\d)(:)?(\\d\\d))",
      "required": true
    },
    "time": {
      "type": "integer",
      "minimum": 0,
      "required": true
    },
    "request": {
      "$ref": "requestType",
      "required": true
    },
    "response": {
      "$ref": "responseType",
      "required": true
    },
    "cache": {
      "$ref": "cacheType",
      "required": true
    },
    "timings": {
      "$ref": "timingsType",
      "required": true
    },
    "serverIPAddress": {
      "type": "string"
    },
    "connection": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    }
  }
}